/* Header Spacing */
@headerTopMargin: e(%("calc(2rem - %d)", @headerLineHeightOffset));
@headerBottomMargin: 1rem;
@headerMargin: @headerTopMargin 0 @headerBottomMargin;

/* Minimum Mobile Width */
