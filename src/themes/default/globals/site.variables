/*******************************
         Site Settings
*******************************/

@import "variation.variables";
@supportIE: true;

/* -------------------
       Fonts
-------------------- */

@fontName: "Lato";
@fontFileNameLatin: "LatoLatin";
@fontFileNameSupplement: "Lato";

