@fontPath: "../../themes/basic/assets/fonts";

@fonts: {
    @solid: {
        font-family: @fontName;
        src: url("@{fontPath}/@{fontName}.woff2") format("woff2") if(@supportIE, e(",") url("@{fontPath}/@{fontName}.woff") format("woff"));
        font-style: normal;
        font-weight: @normal;
        font-variant: normal;
        text-decoration: inherit;
        text-transform: none;
    };
};
